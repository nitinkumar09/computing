                    <script>

                        // footer js star
                        const emailInput = document.getElementById('email-input');
                        const subscribeButton = document.getElementById('subscribe-button');
                        const errorMessage = document.getElementById('error-message');

                        subscribeButton.addEventListener('click', function () {
                            if (emailInput.value.trim() === '') {
                                errorMessage.classList.remove('hidden');
                                emailInput.classList.add('error-border');
                            } else {
                                errorMessage.classList.add('hidden');
                                emailInput.classList.remove('error-border');
                                // Add form submission logic here if needed
                            }
                        });

                        // its js for input border transition
                        emailInput.addEventListener('focus', function () {
                            errorMessage.classList.add('hidden');
                            emailInput.classList.remove('error-border');
                        });

                        emailInput.addEventListener('blur', function () {
                            emailInput.classList.remove('error-border');
                        });
                        // footer js end 

                        //footer  logo js start here
                        // JavaScript to add diagonal movement to the logo
                        // JavaScript to add size change and diagonal movement to the logo


                        // js start here for collapse button
                        document.addEventListener("DOMContentLoaded", function () {
                            const buttons = document.querySelectorAll('.toggle-button');
                            let activeSection = null;

                            buttons.forEach(button => {
                                button.addEventListener('click', function (event) {
                                    event.stopPropagation(); // Prevent click event from bubbling up

                                    const targetId = this.getAttribute('data-target');
                                    const targetSection = document.getElementById(targetId);

                                    if (activeSection && activeSection !== targetSection) {
                                        activeSection.classList.add('hidden');
                                        activeSection.classList.remove('block');
                                    }

                                    if (targetSection.classList.contains('hidden')) {
                                        targetSection.classList.remove('hidden');
                                        targetSection.classList.add('block');
                                        activeSection = targetSection;
                                    } else {
                                        targetSection.classList.add('hidden');
                                        targetSection.classList.remove('block');
                                        activeSection = null;
                                    }
                                });
                            });

                            document.addEventListener('click', function () {
                                if (activeSection) {
                                    activeSection.classList.add('hidden');
                                    activeSection.classList.remove('block');
                                    activeSection = null;
                                }
                            });

                            document.querySelectorAll('.card').forEach(card => {
                                card.addEventListener('click', function (event) {
                                    event.stopPropagation(); // Prevent click event from bubbling up
                                });
                            });
                        });

                        // js end here for collapse button

                        const images = document.querySelectorAll('.marquee img'); // Select all images inside the marquee
                        const marqueeWidth = document.querySelector('.marquee').offsetWidth; // Get marquee width
                        let currentPosition = 0; // Starting position

                        function moveImages() {
                            currentPosition -= 1; // Move images left by 1 pixel on each loop

                            images.forEach((image, index) => {
                                const imageWidth = image.offsetWidth; // Get individual image width
                                const newPosition = currentPosition - index * imageWidth; // Calculate new position considering image width

                                // Check if image needs to reappear from the right
                                if (newPosition < -imageWidth) {
                                    newPosition = marqueeWidth - imageWidth; // Set new position at the end of the marquee
                                }

                                image.style.transform = `translateX(${newPosition}px)`; // Apply the new position
                            });

                            requestAnimationFrame(moveImages); // Request a new animation frame for continuous movement
                        }

                        moveImages(); // Start the animation


                    </script>
