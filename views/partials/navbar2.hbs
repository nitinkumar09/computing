    <script>

        /*NAVBAR JS START HERE*/


        //  start here js for automatically show ('component attached with nav bar') after five second
        const attached = document.getElementById('attached');

        // Use setTimeout to delay for 5 seconds
        setTimeout(() => {
            attached.classList.remove('hidden');
        }, 5000);

        //  end here js for automatically show ('component attached with nav bar') after five second



        // mobile me link collapse end here


        const dropdownButtons = document.querySelectorAll('#mobile-menu .relative > button');

        dropdownButtons.forEach(button => {
            button.addEventListener('click', function () {
                const clickedDropdown = this.nextElementSibling; // Get clicked dropdown

                // Toggle visibility of clicked dropdown
                clickedDropdown.classList.toggle('hidden');

                // Find all parent elements with class 'mobile-menu'
                const parentMenu = this.closest('#mobile-menu');

                // Find all sibling dropdown buttons within the same parent menu
                const siblingButtons = parentMenu.querySelectorAll('.relative > button');

                // Hide dropdowns of sibling buttons (excluding clicked one)
                siblingButtons.forEach(siblingButton => {
                    if (siblingButton !== this) {
                        const siblingDropdown = siblingButton.nextElementSibling;
                        siblingDropdown.classList.add('hidden');
                    }
                });
            });
        });



        // mobile me link collapse end here


        // hamber button functionlity start
        const menuToggle = document.getElementById('menu-toggle');
        const mobileMenu = document.getElementById('mobile-menu');

        menuToggle.addEventListener('click', () => {
            mobileMenu.classList.toggle('hidden');
            menuToggle.classList.toggle('toggle-menu');
        });

        document.querySelectorAll('.plus-icon').forEach(button => {
            button.addEventListener('click', (e) => {
                const submenu = button.parentElement.nextElementSibling;
                submenu.classList.toggle('hidden');
                button.querySelector('i').classList.toggle('fa-plus');
                button.querySelector('i').classList.toggle('fa-times');
            });
        });

        // hamberger button functiolity end


        // js start here for rotate service items icon
        const rotateiconElements = document.querySelectorAll('.rotateicon');

        rotateiconElements.forEach(element => {
            element.addEventListener('mouseenter', () => {
                element.querySelector('i').classList.add('rotate-90');
                element.querySelector('a').classList.add('text-blue-600');
            });

            element.addEventListener('mouseleave', () => {
                element.querySelector('i').classList.remove('rotate-90');
                element.querySelector('a').classList.remove('text-blue-600');
            });
        });
        // js end here for rotate service items icon


        /*NAVBAR JS END HERE*/




    </script>
